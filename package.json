{
  "name": "random-mua",
  "description": "Random Modern User-Agent.",
  "author": "Yahtnif <yahtnif@gmail.com>",
  "version": "0.2.0",
  "main": "dist/index.js",
  "repository": "yahtnif/random-mua",
  "homepage": "https://github.com/yahtnif/random-mua",
  "license": "MIT",
  "keywords": [
    "user-agent",
    "useragent",
    "browser",
    "random"
  ],
  "scripts": {
    "build": "yarn decompress && rollup --c",
    "decompress": "node src/decompress.js",
    "pass": "yarn build && yarn test",
    "test": "jest",
    "version": "yarn pass"
  },
  "lint-staged": {
    "{src|__tests__}/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn pass",
      "pre-push": "yarn pass"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "gunzip-file": "^0.1.1",
    "husky": "^1.2.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.3",
    "rollup": "^0.67.4",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "dependencies": {
    "park-miller": "^1.0.0"
  }
}
